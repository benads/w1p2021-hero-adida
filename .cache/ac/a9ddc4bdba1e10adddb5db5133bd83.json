on: fadeOut 1s;\n}\n\n\n\n/*\n.play-enter-active {\n  animation: myFade 3s infinite;        // a rajouter a la fin \n}*/\n\n$hetic-green: #14133f;\n\n@mixin flex-center { \n  display: flex;\n  align-items: center;\n  justify-content: center;\n}\n\n@keyframes myFade {\n  0%{\n    opacity: 1;\n  }\n\n  25% {\n    opacity: 0;\n  }\n\n  50% {\n    opacity: 1;\n  }\n\n  75% {\n    opacity: 0;\n  }\n\n  100% {\n    opacity: 1;\n  }\n}\n\n\n@keyframes fadeIn {\n  from {\n    opacity: 0;\n  }\n\n  to {\n    opacity: .9;\n  }\n}\n\n\n@keyframes fadeInS {\n  from {\n    opacity: 0;\n    width: 100vw;\n    height: 100vh;\n    background-color: black;\n   transform: translateX(100%);\n  }\n\n  to {\n    opacity: .9; width: 100vw;\n    height: 100vh;\n    background-color: black;\n     \n  }\n}\n\n\n@keyframes fadeOut {\n  from {\n    opacity: .9;\n  }\n\n  to {\n    opacity: 0;\n  }\n}\n\n\nbody,\nbutton,\ninput,\nselect,\ntextarea {\n  font-family: 'ea_font_v1.5_by_ghettosharkRg';\n  text-transform: uppercase;\n  letter-spacing: 1.2px;\n}\n\n.big-header {\n  background-color: $hetic-green;\n  height: 100vh;\n  display: flex;\n  justify-content: space-around;\n  align-items: center;\n\n  flex-direction: column;\n  color: white;\n}\n\n.button {\n  color: $hetic-green;\n  text-decoration: none;\n  background: rgb(34,193,195);\n  background: linear-gradient(0deg, rgba(34,193,195,1) 0%, rgba(253,187,45,1) 100%);\n  padding: 5px;\n  border-radius: 5px;\n  border: 3px purple solid;\n  width: auto;\n  height: 40px;\n  @include flex-center;\n  flex-direction: column;\n\n}\n\n.answers {\n  display: flex;\n  align-items: center;\n  justify-content: space-around;\n  width: 70%;\n}\n\n\n\na img {\n\n  width: 100px;\n  height: 100px;\n}\n\n.hover:hover {\n  /*border-radius: 25px; border: 1px solid blue;*/\n  opacity: .3;\n  cursor: pointer;\n\n \n}\n\n.test {\n  width: 50vw;\n  height: 70%;\n  display: flex;\n  justify-content: space-around;\n  flex-direction: column;\n  align-items: center;\n}\n\n.test .button {\n  width: 50%;\n}\n\n.fade-enter-active, .fade-leave-active {\n  transition: opacity .5s;\n}\n.fade-enter, .fade-leave-to /* .fade-leave-active below version 2.1.8 */ {\n  opacity: 0;\n}\n\n\n\n\n\n@keyframes flipInX {\n  from {\n    -webkit-transform: perspective(400px) rotate3d(1, 0, 0, 90deg);\n    transform: perspective(400px) rotate3d(1, 0, 0, 90deg);\n    -webkit-animation-timing-function: ease-in;\n    animation-timing-function: ease-in;\n    opacity: 0;\n  }\n\n  40% {\n    -webkit-transform: perspective(400px) rotate3d(1, 0, 0, -20deg);\n    transform: perspective(400px) rotate3d(1, 0, 0, -20deg);\n    -webkit-animation-timing-function: ease-in;\n    animation-timing-function: ease-in;\n  }\n\n  60% {\n    -webkit-transform: perspective(400px) rotate3d(1, 0, 0, 10deg);\n    transform: perspective(400px) rotate3d(1, 0, 0, 10deg);\n    opacity: 1;\n  }\n\n  80% {\n    -webkit-transform: perspective(400px) rotate3d(1, 0, 0, -5deg);\n    transform: perspective(400px) rotate3d(1, 0, 0, -5deg);\n  }\n\n  to {\n    -webkit-transform: perspective(400px);\n    transform: perspective(400px);\n  }\n}\n\n.flipInX {\n\n  animation: flipInX 2s ease-in-out;\n}\n\n\n\n\n\n@keyframes fadeOutUp {\n  from {\n    opacity: 1;\n  }\n\n  to {\n    opacity: 1;\n    -webkit-transform: translate3d(420px, -270px, 0);\n    transform: translateY(0,0 , 0);\n  }\n}\n\n\n.translateX {\n  animation: fadeOutUp 2s forwards;\n  \n}\n\n\n\n\n","assets/scss/fonts.scss":"@font-face {\n  font-family: 'ea_font_v1.5_by_ghettosharkRg';\n  src: url('easports15-webfont.woff2') format('woff2'),\n       url('easports15-webfont.woff') format('woff');\n  font-weight: normal;\n  font-style: normal;\n\n}\n\n\n\n\n\n"},"lineCount":null}},"error":null,"hash":"e6924521ed96cf64e63c70c7f152ef95","cacheData":{"env":{}}}